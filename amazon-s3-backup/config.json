{
    "name": "S3 Backup",
    "version": "1.2.1",
    "slug": "s3-backup",
    "description": "Sync Backups to your S3 bucket",
    "url": "https://github.com/newcbomb/hass-addons/tree/main/amazon-s3-backup",
    "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
    "boot": "manual",
    "init": false,
    "startup": "once",
    "advanced": true,
    "hassio_api": true,
    "hassio_role": "backup",
    "options": {
      "aws_access_key": "",
      "aws_secret_access_key": "",
      "custom_endpoint": "",
      "bucket_name": "",
      "bucket_region": "us-west-001",
      "delete_local_backups": true,
      "local_backups_to_keep": 3
    },
    "schema": {
      "aws_access_key": "str",
      "aws_secret_access_key": "password",
      "custom_endpoint": "str",
      "bucket_name": "str",
      "bucket_region": "str",
      "delete_local_backups": "bool",
      "local_backups_to_keep": "int"
    },
    "map": ["backup:rw"]
  }
